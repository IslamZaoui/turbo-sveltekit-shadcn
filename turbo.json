{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"lint": {},
		"lint:fix": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".svelte-kit/**", ".vercel/**"]
		},
		"preview": {
			"cache": false,
			"persistent": false
		}
	}
}
